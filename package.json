{
  "name": "traceable",
  "description": "Trace a state of objects using event streams",
  "version": "1.0.0",
  "author": "Lihai Ben-Haim <lihaibh@gmail.com>",
  "_moduleAliases": {
    "@app": "./src"
  },
  "bugs": {
    "url": "https://github.com/lihaibh/traceable/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "module-alias": "^2.0.6",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.6",
    "typescript-memoize": "^1.0.0-alpha.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^22.1.3",
    "@types/lodash": "^4.14.105",
    "@types/node": "^9.4.7",
    "@types/ramda": "^0.25.18",
    "@types/should": "^13.0.0",
    "@types/sinon": "^4.1.3",
    "commitizen": "^2.9.6",
    "reflect-metadata": "^0.1.12",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "should": "^13.2.1",
    "sinon": "^4.3.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-clean-code": "^0.2.5",
    "tslint-loader": "^3.6.0",
    "tslint-sonarts": "^1.6.0",
    "typescript": "^2.7.2"
  },
  "homepage": "https://github.com/lihaibh/traceable#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "^@lib/(.*)": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  },
  "keywords": [
    "nodejs"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lihaibh/traceable.git"
  },
  "scripts": {
    "commit": "git-cz",
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
